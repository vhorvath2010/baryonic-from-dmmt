    "import torch\n",
    "\n",
    "graphs = torch.load('datasets/unpruned/SG256_SM_Only.pt')\n",
    "\n",
    "invalid = 0\n",
    "for graph in graphs:\n",
    "  if len(graph.x) != len(graph.edge_index[0]) + 1:\n",
    "    invalid += 1\n",
    "\n",
    "print(f'{invalid} of {len(graphs)} graphs were not valid trees')"
   ]
